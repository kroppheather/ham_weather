combine_csv_files <- function(dirData) {
# Get a list of all CSV files in the directory
file_list <- list.files(path = dirData, pattern = "\\.csv$", full.names = TRUE)
# Read and combine all CSV files
combined_data <- do.call(rbind, lapply(file_list, read.csv, stringsAsFactors = FALSE))
return(combined_data)
}
